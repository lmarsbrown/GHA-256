function hash(input) { output = ""; inAr=input.match(/.{1,4}/g); for(let i = 0;i<inAr.length;i++) { output = stringNumXOR(hashBlock(inAr[i]),output); } return(output); function toNum(input) { let output = 0; for(let i = 0;i<input.length;i++) { output = (output*1000)+input.charCodeAt(i); } return output; } function multiAll(input) { let output = 1; for(let i = 0;i<input.length;i++) { output = output*input.charCodeAt(i); } return output; } function hashBlock(input) { let output = ""; let temp = toNum(input); let prev = temp; let prev2=prev; for(let i = 0;i<256;i++) { prev2= prev; prev = temp; temp =parseInt(String(temp*prev2+3)[0]+String(temp*prev2+3)[1],10); output = output+String(temp); } return output; } function stringNumXOR(input1,input2) { output = ""; for(let i = 0;i<input1.length;i++) { output = output+String(Number(input1[i])^Number(input2[i])); } return output } } document.write(hash(prompt()));
